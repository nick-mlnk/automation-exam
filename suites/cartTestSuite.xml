<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="End To End Testing App" parallel="tests" thread-count="2">

    <test verbose="1" name="Unlogged User Is Able To Add Products To Cart Test: Chrome">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="web.UnloggedUserIsAbleToAddProductsToCartTest"></class>
        </classes>
    </test>

    <test verbose="1" name="LoggedIn User Is Able To Add Products To Cart Test: Opera">
        <parameter name="browser" value="opera"/>
        <classes>
            <class name="web.LoggedInUserIsAbleToAddProductsToCartTest"></class>
        </classes>
    </test>

</suite>